{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "FlowR.Core/FlowR.Core.csproj",
            "FlowR.UI/FlowR.UI.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "index.md",
          "toc.yml"
        ]
      },
      {
        "files": [
          "content/**.md",
          "content/*.md",
          "content/toc.yml",
          "content/**/toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "dest": "../docs",
    "globalMetadata": {
      "_appFooter": "Â© 2020 Francesco Danti aka Abbadon1334"
    },
    "globalMetadataFiles": [],
    "fileMetadata": {},
    "template": [
      "default"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}