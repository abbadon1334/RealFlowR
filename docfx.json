{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "FlowR.Core/FlowR.Core.csproj",
            "FlowR.UI/FlowR.UI.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "docfx/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docfx/api/**.yml",
          "docfx/api/index.md",
          "docfx/index.md",
          "docfx/toc.yml"
        ]
      },
      {
        "files": [
          "docfx/content/**.md",
          "docfx/content/*.md",
          "docfx/content/toc.yml",
          "docfx/content/**/toc.yml",
          "docfx/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "docfx/images/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "dest": "docs",
    "globalMetadata": {},
    "globalMetadataFiles": [],
    "fileMetadata": {},
    "template": [],
    "markdownEngineName": "markdig"
  }
}