{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "FlowR.Core/FlowR.Core.csproj",
            "FlowR.UI/FlowR.UI.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "content/**/toc.yml",
          "content/**.md",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "dest": "../docs",
    "globalMetadata": {
      "_appFooter": "Â© 2020 Francesco Danti aka Abbadon1334"
    },
    "globalMetadataFiles": [],
    "fileMetadata": {},
    "template": [
      "default",
      "FlowR.DocFx/DocFx.Plugins.PlantUml/template"
    ],
    "markdownEngineProperties": {
      "plantUml.renderingMode": "local",
      //"plantUml.outputFormat": "png",
      "plantUml.localPlantUmlPath": "DocFx.Plugins.PlantUml/plantuml.jar",
      //"plantUml.remoteUrl": "http://www.plantuml.com/plantuml/",
      //"plantUml.renderingMode": "remote"
    },
    "noLangKeyword": false
  }
}
